import { render, screen } from "@testing-library/react";
import { {{pascalCase name}} } from "./index";

describe("{{pascalCase name}}", () => {
  it("should render the heading", async () => {
    render(
      <{{pascalCase name}} />
    );
    expect(
      screen.getByRole("heading", { name: /{{pascalCase name}}/i })
    ).toBeInTheDocument();
  });
});